{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.18.0",
		"@auth/sveltekit": "0.3.11",
		"@grpc/grpc-js": "1.9.9",
		"@picocss/pico": "1.5.10",
		"@playwright/test": "1.39.0",
		"@protobuf-ts/grpc-transport": "2.9.1",
		"@protobuf-ts/runtime": "2.9.1",
		"@protobuf-ts/runtime-rpc": "2.9.1",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/kit": "1.27.4",
		"@types/node": "20.9.0",
		"@typescript-eslint/eslint-plugin": "6.10.0",
		"@typescript-eslint/parser": "6.10.0",
		"autoprefixer": "10.4.16",
		"depcheck": "1.4.7",
		"eslint": "8.53.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.35.0",
		"just-clone": "6.2.0",
		"material-symbols": "0.14.0",
		"next-auth": "4.24.5",
		"postcss": "8.4.31",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.1.0",
		"sass": "1.69.5",
		"svelte": "4.2.3",
		"svelte-check": "3.6.0",
		"tailwindcss": "3.3.5",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.0",
		"vite-imagetools": "6.2.3",
		"vitest": "0.34.6"
	},
	"type": "module"
}
