{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"semi": true,
	"svelteIndentScriptAndStyle": true,
	"endOfLine": "lf",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"importOrder": ["^@.*$", "^[./]"]
}
