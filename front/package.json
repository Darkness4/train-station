{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.18.6",
		"@auth/sveltekit": "0.5.0",
		"@grpc/grpc-js": "1.9.13",
		"@picocss/pico": "1.5.10",
		"@playwright/test": "1.40.1",
		"@protobuf-ts/grpc-transport": "2.9.3",
		"@protobuf-ts/runtime": "2.9.3",
		"@protobuf-ts/runtime-rpc": "2.9.3",
		"@sveltejs/adapter-node": "2.0.2",
		"@sveltejs/kit": "2.0.6",
		"@sveltejs/vite-plugin-svelte": "3.0.1",
		"@types/node": "20.10.5",
		"@typescript-eslint/eslint-plugin": "6.15.0",
		"@typescript-eslint/parser": "6.15.0",
		"autoprefixer": "10.4.16",
		"depcheck": "1.4.7",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"just-clone": "6.2.0",
		"material-symbols": "0.14.3",
		"next-auth": "4.24.5",
		"postcss": "8.4.32",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2",
		"sass": "1.69.5",
		"svelte": "4.2.8",
		"svelte-check": "3.6.2",
		"tailwindcss": "3.3.7",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.10",
		"vite-imagetools": "6.2.8",
		"vitest": "1.0.4"
	},
	"type": "module"
}
