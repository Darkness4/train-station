{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.24.0",
		"@auth/sveltekit": "0.10.0",
		"@grpc/grpc-js": "1.9.14",
		"@picocss/pico": "1.5.11",
		"@playwright/test": "1.41.1",
		"@protobuf-ts/grpc-transport": "2.9.3",
		"@protobuf-ts/runtime": "2.9.3",
		"@protobuf-ts/runtime-rpc": "2.9.3",
		"@sveltejs/adapter-node": "4.0.1",
		"@sveltejs/kit": "2.5.0",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@types/node": "20.11.8",
		"@typescript-eslint/eslint-plugin": "6.19.1",
		"@typescript-eslint/parser": "6.19.1",
		"autoprefixer": "10.4.17",
		"depcheck": "1.4.7",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"just-clone": "6.2.0",
		"material-symbols": "0.14.6",
		"next-auth": "4.24.5",
		"postcss": "8.4.33",
		"prettier": "3.2.4",
		"prettier-plugin-svelte": "3.1.2",
		"sass": "1.70.0",
		"svelte": "4.2.9",
		"svelte-check": "3.6.3",
		"tailwindcss": "3.4.1",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.12",
		"vite-imagetools": "6.2.9",
		"vitest": "1.2.2"
	},
	"type": "module"
}
