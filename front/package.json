{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.36.0",
		"@auth/sveltekit": "1.6.0",
		"@grpc/grpc-js": "1.12.0",
		"@picocss/pico": "2.0.6",
		"@playwright/test": "1.47.2",
		"@protobuf-ts/grpc-transport": "2.9.4",
		"@protobuf-ts/runtime": "2.9.4",
		"@protobuf-ts/runtime-rpc": "2.9.4",
		"@sveltejs/adapter-node": "5.2.5",
		"@sveltejs/kit": "2.6.2",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/node": "20.16.10",
		"@typescript-eslint/eslint-plugin": "8.8.0",
		"@typescript-eslint/parser": "8.8.0",
		"autoprefixer": "10.4.20",
		"depcheck": "1.4.7",
		"eslint": "9.12.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.44.1",
		"just-clone": "6.2.0",
		"material-symbols": "0.24.0",
		"next-auth": "4.24.8",
		"postcss": "8.4.47",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.7",
		"sass": "1.79.4",
		"svelte": "4.2.19",
		"svelte-check": "3.8.6",
		"tailwindcss": "3.4.13",
		"tslib": "2.7.0",
		"typescript": "5.6.2",
		"vite": "5.4.8",
		"vite-imagetools": "7.0.4",
		"vitest": "2.1.2"
	},
	"type": "module"
}
