{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.28.0",
		"@auth/sveltekit": "0.14.0",
		"@grpc/grpc-js": "1.10.3",
		"@picocss/pico": "2.0.6",
		"@playwright/test": "1.42.1",
		"@protobuf-ts/grpc-transport": "2.9.4",
		"@protobuf-ts/runtime": "2.9.4",
		"@protobuf-ts/runtime-rpc": "2.9.4",
		"@sveltejs/adapter-node": "4.0.1",
		"@sveltejs/kit": "2.5.4",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@types/node": "20.11.30",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"autoprefixer": "10.4.19",
		"depcheck": "1.4.7",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"just-clone": "6.2.0",
		"material-symbols": "0.17.0",
		"next-auth": "4.24.7",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.2",
		"sass": "1.72.0",
		"svelte": "4.2.12",
		"svelte-check": "3.6.7",
		"tailwindcss": "3.4.1",
		"tslib": "2.6.2",
		"typescript": "5.4.2",
		"vite": "5.1.6",
		"vite-imagetools": "6.2.9",
		"vitest": "1.4.0"
	},
	"type": "module"
}
