{
	"name": "train-station-front",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"gen:e2e": "playwright codegen --load-storage=playwright/.auth/auth.json http://localhost:4173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"depcheck": "depcheck"
	},
	"trustedDependencies": [
		"sharp"
	],
	"devDependencies": {
		"@auth/core": "0.31.0",
		"@auth/sveltekit": "1.1.0",
		"@grpc/grpc-js": "1.10.8",
		"@picocss/pico": "2.0.6",
		"@playwright/test": "1.44.1",
		"@protobuf-ts/grpc-transport": "2.9.4",
		"@protobuf-ts/runtime": "2.9.4",
		"@protobuf-ts/runtime-rpc": "2.9.4",
		"@sveltejs/adapter-node": "5.0.1",
		"@sveltejs/kit": "2.5.10",
		"@sveltejs/vite-plugin-svelte": "3.1.0",
		"@types/node": "20.12.12",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"autoprefixer": "10.4.19",
		"depcheck": "1.4.7",
		"eslint": "9.3.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.0",
		"just-clone": "6.2.0",
		"material-symbols": "0.18.0",
		"next-auth": "4.24.7",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.3",
		"sass": "1.77.2",
		"svelte": "4.2.17",
		"svelte-check": "3.7.1",
		"tailwindcss": "3.4.3",
		"tslib": "2.6.2",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"vite-imagetools": "7.0.2",
		"vitest": "1.6.0"
	},
	"type": "module"
}
